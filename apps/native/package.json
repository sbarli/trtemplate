{
  "name": "@changeme/native",
  "version": "0.0.0",
  "description": "ReactNative/Expo application",
  "author": "Samantha Barli",
  "main": "index.js",
  "scripts": {
    "start": "doppler run -- expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "dev": "yarn start",
    "predev": "cd ../../ && turbo run @changeme/types#build && turbo run @changeme/gql#build && turbo run @changeme/utils#build",
    "eas": "eas",
    "exp": "npx expo",
    "exp:doc": "npx expo-doctor@latest",
    "lint:types": "yarn tsc",
    "lint:eslint": "eslint \"**/*.{ts,tsx,js,jsx}\"",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:fix": "yarn lint:eslint:fix"
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@expo/webpack-config": "^19.0.0",
    "@gluestack-style/react": "^1.0.8",
    "@gluestack-ui/config": "^1.0.3",
    "@gluestack-ui/themed": "^1.0.8",
    "@hookform/resolvers": "^3.3.2",
    "@changeme/types": "*",
    "@changeme/gql": "*",
    "@changeme/ui": "*",
    "@changeme/utils": "*",
    "@react-native-async-storage/async-storage": "1.18.2",
    "expo": "^49.0.0",
    "expo-clipboard": "~4.3.1",
    "expo-constants": "~14.4.2",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-status-bar": "~1.6.0",
    "graphql": "^16.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.48.2",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-web": "~0.19.6",
    "socket.io-client": "^4.7.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "eslint-config-custom": "*",
    "typescript": "^5.1.3"
  },
  "private": true
}
